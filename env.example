# Family Dinner Picker Environment Variables
# Copy this file to .env and set your password

# Password for accessing the app (required)
DINNER_PASSWORD=your-secret-password-here

# Family members (comma-separated, will be sorted alphabetically)
FAMILY_MEMBERS=Bob,Sue,John,Alice

# Supabase Database Configuration (required for persistent storage)
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Environment (development/production) - defaults to development
NODE_ENV=development

# Optional: Override default port (defaults to 3000)
# PORT=3000 